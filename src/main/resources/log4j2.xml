<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout pattern="%d %p %c{1.} %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <root level="info">
            <AppenderRef ref="STDOUT" level="debug"/>
        </root>
    </Loggers>
</Configuration>